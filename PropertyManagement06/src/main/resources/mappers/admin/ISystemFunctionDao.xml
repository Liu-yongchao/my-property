<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 系统模块ISystemFunctionDao接口Mapping映射 -->  
<mapper namespace="com.neusoft.wuyemis.admin.dao.ISystemFunctionDao">
	
  <insert id="create" flushCache="true">
   insert into userinfo values (#{funNo},#{mno},#{funName},#{funUrl},#{leveNo}'N')
  
  </insert>
  <update id="update">
  insert into userinfo set UserFunNo=#{funNo},UserMno=#{mno},UserFunName=#{funName},UserFunUrl=#{funUrl}
  UserLeveNo=#{leveNo}
  </update>
 <delete id="delete">
 delete from UserInfo where UserID=#{userinfoId}
 
 </delete>


</mapper>